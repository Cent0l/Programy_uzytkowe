%% Obliczenia
x=-2*pi:0.01:2*pi;
y1=x.*sin(x);
y2=x.*cos(x);
%% Rysujowanie wykresu
figure(1)
hold on
plot(x,y1,'r');
plot(x,y2,'g');
% dodanie legendy
legend('xsin(x)','xcos(x)');
hold off


function [y]=kwadrat(x,a,b,c)
%% [y]=kwadrat(a,b,c)
%% funkcja oblicza wartość funckji kwadratowej ax^2+bx+c
y = a*x^2+b*x+c;
potem
kwadrat(1,2,3,4) itp
